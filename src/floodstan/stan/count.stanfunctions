
// ------- Count functions -----------------------------
// 1=Poisson, 2=Negative Binomial

real count_lpmf(int n, int count, real locn, real phi)
{

    if(count==1){
        return poisson_lpmf(n | locn);
    }
    else if(count==2){
        return neg_binomial_2_lpmf(n | locn, phi);
    }
    return not_a_number();
}



